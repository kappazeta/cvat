{
    "all": true,
    "recursive": true,
    "compact": false,
    "useInlineSourceMaps": false,
    "extension": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx"
    ],
    "exclude": [
        "**/3rdparty/*",
        "**/tests/*"
    ],
    "parser-plugins": [
        "typescript"
    ]
}
